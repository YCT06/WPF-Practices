<Application x:Class="_20241016.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:_20241016"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate x:Key="StationData">
            <StackPanel>
                <TextBlock Text="{Binding StationID}" />
                <TextBlock Text="{Binding StationName}" />
                <TextBlock Text="{Binding IsEquipError}" />
            </StackPanel>
        </DataTemplate>
    </Application.Resources>
</Application>
